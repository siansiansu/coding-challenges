    "class Solution:\n",
    "    def lengthOfLastWord(self, s):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        return len(s.rstrip().split(' ')[-1])\n",
    "\n",
    "if __name__ == \"__main__\":    \n",
    "    print(Solution().lengthOfLastWord('Hello World'))   "
